<h1>Edit <%= @song.title %></h1>
<%= form_for @song, :html => {:class => "form-group"} do |f| %>
   Title <%= f.text_field :title, :placeholder => "", class: "form-control" %>
   Genre <%= f.text_field :genre, :placeholder => "",class: "form-control" %><br>
   Cover Image <%= f.file_field :songpic, class: "form-control" %><br>
   Audio File <%= f.file_field :songpath, class: "form-control" %>
   <%= link_to "Delete Song", song_path, :method => "DELETE", class: "btn btn-danger" %>
<%= f.submit %>
<% end %>